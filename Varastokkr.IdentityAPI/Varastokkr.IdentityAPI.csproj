    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>766fec11-fed5-497f-b8ca-b0bc0b64d38a</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
